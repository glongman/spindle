<plugin
   id="com.iw.plugins.spindle.ui"
   name="Spindle UI plugin"
   version="3.1.25"
   provider-name="Geoffrey Longman"
   class="com.iw.plugins.spindle.UIPlugin">

   <runtime>
      <library name="spindle_ui.jar">
         <export name="*"/>
      </library>
      <library name="xmen.jar"/>
      <library name="lib/commons-io-1.0.jar">
         <export name="*"/>
      </library>
      <library name="lib/jdom.jar">
         <export name="*"/>
      </library>
   </runtime>
